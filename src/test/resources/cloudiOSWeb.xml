<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Native mobile testing Suite">
    <parameter name="appType" value="web"/>
    <parameter name="platformName" value="iOS"/>
    <parameter name="deviceName" value=""/>
    <parameter name="browserName" value="iPhone"/>
    <parameter name="device" value="iPhone 11"/>
    <parameter name="os_version" value="13.0"/>
    <parameter name="realMobile" value="true"/>
    <test name="Cloud iOS Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>